# Terraform openstack subnet
# ----------------------------------------------------------------------------
resource "openstack_networking_subnet_v2" "{{ item_subnet.name }}" {
  name       = "{{ item_subnet.name }}"
  network_id = "${openstack_networking_network_v2.{{ item_subnet.network_name }}.id}"
  cidr       = "{{ item_subnet.cidr }}"
  ip_version = 4
}
